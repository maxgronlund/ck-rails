            <!-- box listing -->
            <div class="block-section-sm box-list-area">

              <!-- desc top -->
              <div class="row hidden-xs">
                <div class="col-sm-6  ">
                  <p><strong class="color-black"></strong></p>
                </div>
                <div class="col-sm-6">
                  <p class="text-right">
                    
                  </p>
                </div>
              </div><!-- end desc top -->

              <!-- item list -->
              <div class="box-list">

                <div id="item-wrapper">
                      <div class="item">
                        <div class="row">
                          <!-- <div class="col-md-1 hidden-sm hidden-xs"><div class="img-item"><img src="<%= @job.user.avatar_url() %>" alt=""></div></div> -->
                          <div class="col-md-9">
                            <h3 class="no-margin-top"><a href="/job/<%= @job.job_hash_id %>" class=""><%= @job.job_name %><i class="fa fa-link color-white-mute font-1x"></i></a></h3>
                            <h5><span class="color-black"><%= @job.state.state_name %></span> - <span class="color-white-mute"><%= @job.city.city_name %></span></h5>
                            <p class="text-truncate "><%= @job.salary.salary_string %></p>
                            <div>
                              <span class="color-white-mute time"><%= @job.created_at.to_date %></span>
                            </div>
                          </div>
                          <div class="col-md-2">
                            <img src="<%= @job.user.avatar_url() %>" alt="" width="150" height="150">
                          </div>
                        </div>
                        <div class="row">
                          <div class="col-md-12">
                            <p><%= raw @job.job_description %></p>
                          </div>
                        </div>
                      </div>
                </div>



              </div>

              <!-- pagination -->

            </div><!-- end box listing -->


          </div>
          <div class="col-md-3">
