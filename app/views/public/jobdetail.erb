        <div class="col-md-8">
          <!-- form search -->
          <form class="form-search-list" method="get" action="/job">
            <div class="row">
              <div class="col-sm-5 col-xs-6 ">
                <div class="form-group">
                  <label class="color-white">Apa</label>
                  <input name="query" id="jobquery" class="form-control" placeholder="job title, keywords or company name" >
                </div>
              </div>
              <div class="col-sm-5 col-xs-6 ">
                <div class="form-group">
                  <label class="color-white">Dimana</label>
                  <!--<input class="form-control" placeholder="city, province, or region">-->
                  <select class="form-control" id="jobloc" name="loc">
                    <option value="0">Semua</option>
                    <% @states.each do |st| %>
                        <option value="<%= st.id %>"><%= st.state_name %></option>
                    <% end %>
                  </select>
                </div>
              </div>
              <div class="col-sm-2 col-xs-12 ">
                <div class="form-group">
                  <label class="hidden-xs">&nbsp;</label>
                  <button id="searchbtn" class="btn btn-block btn-theme btn-success">Search</button>
                </div>
              </div>
            </div>
          </form>  <!-- form search -->
